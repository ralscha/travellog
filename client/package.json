    "@angular/platform-browser-dynamic": "17.1.1",
    "@angular/pwa": "17.1.1",
    "@angular/router": "17.1.1",
    "@angular/service-worker": "17.1.1",
    "@ionic/angular": "7.6.6",
    "ionicons": "7.2.2",
    "date-fns": "3.3.1",
    "dexie": "3.2.4",
    "pwacompat": "2.0.17",
