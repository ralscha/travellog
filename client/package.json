{
  "name": "travellog",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prebuild-prod": "node ./build-info-env.js",
    "build-prod": "ng build --prod --baseHref=/app/",
    "postbuild-prod": "bread-compressor dist/app",
    "serve-dist": "ws --hostname localhost -d dist/app -p 1234 -o --log.format tiny --rewrite \"/be/* -> http://localhost:8080/be/$1\"",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.0.9",
    "@angular/common": "10.0.9",
    "@angular/compiler": "10.0.9",
    "@angular/core": "10.0.9",
    "@angular/forms": "10.0.9",
    "@angular/platform-browser": "10.0.9",
    "@angular/platform-browser-dynamic": "10.0.9",
    "@angular/pwa": "0.1000.6",
    "@angular/router": "10.0.9",
    "@angular/service-worker": "10.0.9",
    "@ionic/angular": "5.3.1",
    "date-fns": "2.15.0",
    "dexie": "3.0.2",
    "ngx-custom-validators": "9.1.0",
    "pwacompat": "2.0.15",
    "rxjs": "6.6.2",
    "tslib": "2.0.1",
    "ua-parser-js": "0.7.21",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.6",
    "@angular/cli": "10.0.6",
    "@angular/compiler-cli": "10.0.9",
    "@fortawesome/fontawesome-pro": "5.14.0",
    "@ionic/angular-toolkit": "2.3.0",
    "@types/node": "14.0.27",
    "bread-compressor-cli": "1.1.0",
    "codelyzer": "6.0.0",
    "local-web-server": "4.2.1",
    "replace-in-file": "6.1.0",
    "tslint": "6.1.3",
    "typescript": "3.9.7"
  }
}
