    "@angular/pwa": "17.3.10",
    "@angular/router": "17.3.10",
    "@angular/service-worker": "17.3.10",
    "@ionic/angular": "7.8.6",
    "ionicons": "7.4.0",
    "date-fns": "4.1.0",
