    "@angular/platform-browser": "13.3.10",
    "@angular/platform-browser-dynamic": "13.3.10",
    "@angular/pwa": "13.3.10",
    "@angular/router": "13.3.10",
    "@angular/service-worker": "13.3.10",
    "@ionic/angular": "6.1.7",
    "date-fns": "2.28.0",
    "dexie": "3.2.2",
    "ngx-custom-validators": "11.0.1",
    "pwacompat": "2.0.17",
    "rxjs": "7.5.5",
