    "@fortawesome/fontawesome-free": "5.15.4",
    "@ionic/angular-toolkit": "5.0.3",
    "@types/node": "16.11.12",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "bread-compressor-cli": "1.1.0",
