    "@angular/compiler-cli": "13.3.10",
    "@fortawesome/fontawesome-free": "6.1.1",
    "@ionic/angular-toolkit": "6.1.0",
    "@types/node": "17.0.36",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "bread-compressor-cli": "1.1.0",
    "eslint": "8.16.0",
