    "@angular/forms": "12.2.8",
    "@angular/platform-browser": "12.2.8",
    "@angular/platform-browser-dynamic": "12.2.8",
    "@angular/pwa": "12.2.8",
    "@angular/router": "12.2.8",
    "@angular/service-worker": "12.2.8",
    "@ionic/angular": "5.8.1",
    "date-fns": "2.24.0",
    "dexie": "3.0.3",
    "ngx-custom-validators": "11.0.1",
    "pwacompat": "2.0.17",
    "rxjs": "7.3.1",
    "tslib": "2.3.1",
    "ua-parser-js": "0.7.28",
    "zone.js": "~0.11.4"
