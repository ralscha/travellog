{
  "name": "travellog",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prebuild-prod": "node ./build-info-env.js",
    "build-prod": "ng build --prod --baseHref=/app/",
    "postbuild-prod": "bread-compressor dist/app",
    "serve-dist": "ws --hostname localhost -d dist/app -p 1234 -o --log.format tiny --rewrite \"/be/* -> http://localhost:8080/be/$1\"",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.8",
    "@angular/common": "11.2.8",
    "@angular/compiler": "11.2.8",
    "@angular/core": "11.2.8",
    "@angular/forms": "11.2.8",
    "@angular/platform-browser": "11.2.8",
    "@angular/platform-browser-dynamic": "11.2.8",
    "@angular/pwa": "0.1102.7",
    "@angular/router": "11.2.8",
    "@angular/service-worker": "11.2.8",
    "@ionic/angular": "5.6.3",
    "date-fns": "2.19.0",
    "dexie": "3.0.3",
    "ngx-custom-validators": "11.0.1",
    "pwacompat": "2.0.17",
    "rxjs": "6.6.7",
    "tslib": "2.1.0",
    "ua-parser-js": "0.7.23",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.7",
    "@angular/cli": "11.2.7",
    "@angular/compiler-cli": "11.2.8",
    "@fortawesome/fontawesome-free": "5.15.1",
    "@ionic/angular-toolkit": "3.1.1",
    "@types/node": "14.14.37",
    "bread-compressor-cli": "1.1.0",
    "codelyzer": "6.0.1",
    "local-web-server": "4.2.1",
    "replace-in-file": "6.2.0",
    "tslint": "6.1.3",
    "typescript": "4.1.5"
  }
}
