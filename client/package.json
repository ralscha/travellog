{
  "name": "travellog",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prebuild-prod": "node ./build-info-env.js",
    "build-prod": "ng build --prod --base-href=/app/",
