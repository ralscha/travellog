    "@fortawesome/fontawesome-free": "6.5.1",
    "@ionic/angular-toolkit": "11.0.0",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "bread-compressor-cli": "2.0.0",
    "eslint": "8.56.0",
